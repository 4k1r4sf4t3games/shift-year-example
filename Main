import java.util.*;
import java.util.Scanner;

public class Main
{
	public static void main(String[] args)
	{
		System.out.println("Bitte geben Sie eine Jahreszahl ein: ");
		Scanner scan = new Scanner(System.in);
		int userEingabe = scan.nextInt();
		for (int i = 1900; i < userEingabe + 1; i++) 
		//startwert i frei waehlbar..auch zb -0050 (also 50vChr.)
		{
			if ((b(i)) == true)
			{
				System.out.println("Das Jahr " + i + " ist ein Schaltjahr!");
			}
			else
			{

			}
		}
	}



//boolean ueberprueft ob i / 4 ohne rest teilbar ODER|| i/100 und i/400 ohne rest teilbar
	// dann is boolean true. ansonaten false und kein schaltjahr
	public static boolean b(int i)
	{
		if (i % 4 == 0 || i % 100 == 0 && i % 400 == 0)
		{

			return true;

		}
		else
		{

			return false;

		}
	}

}
